#include <iostream>

using namespace std;

int main()
{
    int ilosc;
    int n = 0;
    int max = 0;
    cout << "wprowadz dlugosc tablicy" << endl;
    cin >> ilosc;
    int tablica[100];
    for (int i = 0; i < ilosc; i++) {
        cin >> n;
        tablica[i] = n;
    }
    for (int i = 0; i < ilosc; i++) {
        if (tablica[i] > max) {
            max = tablica[i];
        }

    }
    cout << "najwieksza liczba jest" << max << endl;
}
