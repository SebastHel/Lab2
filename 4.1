#include <iostream>

using namespace std;

int main()
{
    int ilosc = 0;
    int n = 0;
    int max = 0;
    cout << "wprowadź długość tablicy" << endl;
    cin >> ilosc;
    int tablica[ilosc];
    for(int i = 0; i<ilosc; i++){
        cin >> n;
        tablica[i] = n;
    }
    for(int i = 0; i<ilosc; i++){
        if(tablica[i] > max){
            max = tablica[i];
        }
            
    }
    cout << "największą liczbą jest" << max << endl;
}
