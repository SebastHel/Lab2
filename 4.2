#include <iostream>

using namespace std;

int main()
{
    int ilosc;
    int n;
    cout << "wprowadz dÅ‚ugosc tablicy" << endl;
    cin >> ilosc;
    int tablica[100];
    for (int i = 0; i < ilosc; i++) {
        cout << "podaj warosc dla elementu" << i << endl;
        cin >> tablica[i];
    }
    int najczesciej = 0;
    int maxcount = 0;

    for (int i = 0; i < ilosc; i++) {
        int count = 0;

        for (int j = 0; j < ilosc; j++) {
            if (tablica[i] == tablica[j])
                count++;

        if (count > maxcount) {
            maxcount = count;
            najczesciej = tablica[i];

        }
    }

    
    cout << "najczestsza liczba jest" << najczesciej << endl;
}
