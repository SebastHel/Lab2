#include <iostream>

using namespace std;

int main()
{
    int a,b,i,j;
    string wyraz ="*";
    cin >> a;
    cin >> b;
    i = 0;
    while(i<b){
        std::cout << wyraz << std::endl;
        i++;
    }
    cout<<"Prostokąt" <<endl;
    
    for (j=0;j<b;j++){
        for(i=0;i<a;i++){
            cout<<"*";
        }
        cout<<endl;
    }
    cout<<"Prostokąt-Pusty" <<endl;
    for (j=0;j<b;j++){
        for(i=0;i<=a;i++){
            if(a>i && i>0 && j>0 && j<b){
                cout<<" ";
            }
            else{
                cout<<"*";
            }
            
        }
        
        cout<<endl;
    }
    
    
    return 0;
}
